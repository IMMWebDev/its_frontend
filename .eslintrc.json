{
  "extends": [
    "eslint:recommended",
    "next",
    "next/core-web-vitals", //Includes Next.js' base ESLint configuration along with a stricter Core Web Vitals rule-set
    "eslint-config-prettier" //Turns off all rules that are unnecessary or might conflict with Prettier.
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn"
  },
  "plugins": ["eslint-plugin-prettier", "@emotion/eslint-plugin"]
}
